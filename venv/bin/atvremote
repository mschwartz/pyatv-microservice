#!/Users/mschwartz/PycharmProjects/untitled/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyatv==0.3.13','console_scripts','atvremote'
__requires__ = 'pyatv==0.3.13'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyatv==0.3.13', 'console_scripts', 'atvremote')()
    )
